{
  "expo": {
    "name": "SMS Reader",
    "slug": "sms-reader",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "smsreader",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "permissions": [
        "READ_SMS",
        "READ_CONTACTS"
      ],
      "package": "com.aurele.smsreader"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aa0f717e-5932-4e4f-a392-e04b77856e1e"
      }
    }
  }
}
